{
    "manifest_version": 3,
    "name": "PDF Data Extractor With Gemini",
    "version": "1.0",
    "description": "Extensão para extrair dados específicos de arquivos PDF.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "pdf-icon.png"
    }
  }
  