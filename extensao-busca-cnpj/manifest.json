{
    "name": "Buscar CNPJ",
    "description": "Busca dados de CNPJ da API ReceitaWS",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab", 
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "cnpj.png"
    }
}