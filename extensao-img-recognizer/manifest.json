{
    "manifest_version": 3,
    "name": "Recognize Image",
    "version": "1.0",
    "description": "Extens√£o para extrair texto de imagem com IA.",
    "side_panel": {
      "default_path": "popup.html"
    },
    "permissions": ["storage", "activeTab", "scripting", "runtime", "sidePanel", "tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "img-icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "open_page": "popup.html"
  }
  