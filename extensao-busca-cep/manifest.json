{
    "name": "Buscar CEP",
    "description": "Busca dados de CEP da API Viacep",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab", 
        "scripting"
    ],
    "host_permissions": [
        "<all urls>"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "cep.png"
    }
}